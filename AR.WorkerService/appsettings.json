{
  "Jobs": {
    // Cron Expressions
    "ExampleJob": "0 * * ? * *", // Every 1 minute
    "ExampleJob2": "0 */2 * ? * *" // Every 2 minutes
  },
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.File" ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Error",
        "System": "Error"
      }
    },
    "WriteTo": [
      { "Name": "Console" },
      {
        "Name": "File",
        "Args": { "path": "Logs/log.txt" }
      }
    ],
    "Enrich": [ "FromLogContext", "WithMachineName", "WithThreadId", "WithExceptionDetails" ],
    "Properties": {
      "Application": "AR.WorkerService"
    }
  }
}
